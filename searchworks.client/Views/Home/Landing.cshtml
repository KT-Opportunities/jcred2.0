@{
    ViewBag.Title = "Landing Page";
}



    <div class="col-md-10 col-sm-10">
        <div class="sea act">SEARCH RESULTS</div>
        <div class="sea_bar">
            <label>Search History</label>
            <input name="search" />
            <button>RESET</button>
        </div>
        <div class="result_bar">
            <ul>
                <li>Search Type</li>
                <li>Description</li>
                <li>Response Type</li>
                <li>Reference</li>
                <li>Time Searched</li>

            </ul>
        </div>
        <div class="con">
            <div class="col-md-12 col-sm-12">
                <div class="no_results">
                    <img src="~/Content/img/Place Holder for search history.png" alt="can't display image">
                    <div class="no_res">
                        No recent searches available. Please modify criteria above.
                    </div>
                </div>


            </div>

        </div>
    </div>
